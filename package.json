{
  "name": "tisktask",
  "version": "1.0.0",
  "node": "11.14.0",
  "description": "Code Bootcamp Project #3",
  "proxy": "http://localhost:8000",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marinemekhakyan/tisktask.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marinemekhakyan/tisktask/issues"
  },
  "homepage": "https://github.com/marinemekhakyan/tisktask#readme",
  "dependencies": {
    "@babel/standalone": "^7.7.3",
    "axios": "^0.19.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.2.0",
    "if-env": "^1.0.4",
    "mysql": "^2.17.1",
    "mysql2": "^2.0.0",
    "react-moment": "^0.9.6",
    "react-router": "^5.1.2",

    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^1.19.4"
  }
}
